{
  "name": "@eyedea/syncano",
  "version": "0.9.0",
  "description": "Custom wrapper for syncano-core library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/eyedea-io/syncano.git",
  "author": "dev@eyedea.io",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "config",
    "src",
    "bin",
    "scripts",
    "socket-template"
  ],
  "bin": {
    "syncano-scripts": "./bin/syncano-scripts.js"
  },
  "engines": {
    "node": ">=8.10"
  },
  "keywords": [
    "syncano",
    "core",
    "socket",
    "endpoint"
  ],
  "scripts": {
    "lint": "tslint {src,test}/**/*.ts",
    "build": "tsc",
    "codecov": "codecov",
    "prebuild": "rimraf ./dist",
    "prepublish": "npm run snyk-protect && npm run build",
    "test": "jest",
    "canary": "npm version prerelease --preid=alpha",
    "postinstall": "node ./scripts/postinstall.js",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@syncano/core": "0.16.0-alpha.0d85dd34",
    "@syncano/test": "0.15.1-alpha.33",
    "@syncano/validate": "0.15.0",
    "@types/bluebird": "3.5.28",
    "@types/jest": "24.0.20",
    "@types/sinon": "7.5.0",
    "babel-jest": "24.9.0",
    "chalk": "2.4.2",
    "cross-spawn": "6.0.5",
    "fs-extra": "8.1.0",
    "jest": "24.9.0",
    "sinon": "7.5.0",
    "snyk": "^1.239.3"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/node": "12.11.7",
    "@types/node-fetch": "2.1.4",
    "codecov": "3.1.0",
    "rimraf": "2.6.2",
    "ts-jest": "24.1.0",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  },
  "prettier": {
    "bracketSpacing": false,
    "semi": false,
    "singleQuote": true
  },
  "snyk": true
}
